    "dotenv-flow": "^4.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "restify": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^14.0.0",
    "@types/restify": "8.5.5",
